{{!
    @template block_myacademicplan/main

    This template renders the main content area for the listacursos_mss block.

    Example context (json):
    {}
}}

<div id="block_myacademicplan-{{uniqid}}" class="block_myacademicplan block-cards" data-region="myacademicplan" role="navigation">

            <div data-region="filter" class="d-flex align-items-center flex-wrap" aria-label="{{#str}} aria:controls, block_myacademicplan {{/str}}">
                {{> block_myacademicplan/nav-grouping-selector }}
            </div>

    <div class="container-fluid p-0">
        {{> block_myacademicplan/courses-view }}
    </div>
</div>
{{#js}}
require(['jquery','block_myacademicplan/main',],
function($,Main) {
    var root = $('#block-myacademicplan-{{uniqid}}');
    Main.init(root);
});
{{/js}}
